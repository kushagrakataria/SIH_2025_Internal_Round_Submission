# Production Environment Variables
# Copy this file to .env.production and fill in your actual values

# Firebase Production Configuration
VITE_FIREBASE_API_KEY=your_production_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_production_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_production_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Application Configuration
VITE_APP_NAME="Safe Traveler Buddy"
VITE_APP_VERSION="1.0.0"
VITE_APP_ENVIRONMENT="production"

# Emergency Services
VITE_EMERGENCY_CONTACT="+91-1234567890"
VITE_EMERGENCY_EMAIL="emergency@safetraveler.com"

# API Configuration (if you have backend APIs)
VITE_API_BASE_URL="https://api.safetraveler.com"
VITE_MAPS_API_KEY="your_maps_api_key"

# Analytics (optional)
VITE_GA_MEASUREMENT_ID="G-XXXXXXXXXX"

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PWA=true
VITE_ENABLE_OFFLINE_MODE=true